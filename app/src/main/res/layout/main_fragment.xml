<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto">
	
	<data>
	
	</data>
	
	<androidx.constraintlayout.widget.ConstraintLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent">
		
		<ir.siriusapps.moneysave.customView.Circle
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:layout_marginTop="80dp"
				app:cr_storkColor="@color/lightGreen"
				app:cr_storkWidth="5dp"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent" />
		
		<ir.siriusapps.moneysave.customView.CircleRec
				android:layout_width="100dp"
				android:layout_height="100dp"
				android:layout_marginTop="80dp"
				app:rcr_endAngel="30"
				app:rcr_StartAngel="-90"
				app:rcr_strokColor="@color/color_yellow"
				app:rcr_strokWidth="5dp"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent" />
		
		<Button
				android:id="@+id/nextButton"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Button"
				app:layout_constraintBottom_toBottomOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintTop_toTopOf="parent" />
	
	</androidx.constraintlayout.widget.ConstraintLayout>
</layout>